{
  "name": "todo_api",
  "version": "1.0.0",
  "description": "Backend for TODO app",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start:dev": "nodemon --inspect server.js",
    "migrate": "./node_modules/.bin/sequelize db:migrate",
    "seed": "./node_modules/.bin/sequelize db:seed:all",
    "lint:check": "./node_modules/.bin/eslint .",
    "lint:fix": "./node_modules/.bin/eslint --fix .",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JustShiba/TODO_API.git"
  },
  "keywords": [
    "express",
    "node",
    "psql",
    "sequelize"
  ],
  "author": "JustShiba (liyalisica9356@gmail.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JustShiba/TODO_API/issues"
  },
  "homepage": "https://github.com/JustShiba/TODO_API#readme",
  "dependencies": {
    "body-parser": "^1.20.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "helmet": "^6.0.0",
    "http": "^0.0.1-security",
    "log4js": "^6.6.1",
    "morgan": "^1.10.0",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.21.4",
    "sequelize-cli": "^6.4.1"
  },
  "devDependencies": {
    "eslint": "8.22.0",
    "nodemon": "^2.0.19",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.7.0",
    "husky": "^8.0.1"
  },
  "lint-staged": {
    "*.js": "eslint --fix"
  }
